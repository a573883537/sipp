{
  "name": "sipp-scenario-designer",
  "version": "1.1.0",
  "description": "Visual editor for SIPp XML scenarios",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "start": "npx serve .",
    "dev": "npx serve . -l 8080",
    "build": "echo 'No build step required - static files only' && npm run validate",
    "validate": "node validate.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "sipp",
    "sip",
    "voip",
    "testing",
    "scenario",
    "editor"
  ],
  "author": "SIPp Contributors",
  "license": "GPL-2.0",
  "devDependencies": {
    "fast-xml-parser": "^4.3.2",
    "vitest": "^1.0.4",
    "jsdom": "^23.0.1",
    "@xmldom/xmldom": "^0.8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SIPp/sipp.git"
  },
  "homepage": "https://github.com/SIPp/sipp"
}
